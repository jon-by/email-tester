    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "send-message"
    },
    {
      "name": "template-criar"
    },
    {
      "name": "template-editar"
    },
    {
      "name": "template-listar"
    },
    {
      "name": "read-workspace-apps-assets"
    },
    {
      "name": "update-app-settings"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
